<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/layout-default-latest.css" rel="stylesheet" />
    <link href="~/Content/sidebar/fontello.css" rel="stylesheet" />
    <link href="~/Content/sidebar/normalize.css" rel="stylesheet" />
    <link href="~/Content/sidebar/sidebar.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/jquery-ui-latest.js"></script>
    <script src="~/Scripts/jquery.layout-latest.js"></script>
    <script src="~/Scripts/jquery.serialize-object.js"></script>
    <script>
        $(document).ready(function () {
            $('body').layout({ applyDefaultStyles: true });
        });
    </script>
</head>

<body>
    @RenderBody()
</body>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</html>


